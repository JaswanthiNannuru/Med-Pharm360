<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidproject">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>


    <application

        android:name=".App"

        android:allowBackup="true"
        android:icon="@mipmap/doctor"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SelectionActivity"> </activity>
        <activity android:name=".DoctorSignupActivity"> </activity>
        <activity android:name=".PatientSignupActivity"> </activity>
        <activity android:name=".PatientHomeActivity"> </activity>
        <activity android:name=".DoctorHomeActivity"> </activity>
        <activity android:name=".DoctordataActivity"> </activity>
        <activity android:name=".HistoryloginActivity"> </activity>
        <activity android:name=".DoctorslistActivity"> </activity>
        <activity android:name=".AppointmentActivity"> </activity>
        <activity android:name=".PatientsrecordActivity"> </activity>
       
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.parse.SERVER_URL"
            android:value="@string/back4app_server_url" />
        <meta-data
            android:name="com.parse.APPLICATION_ID"
            android:value="@string/back4app_app_id" />
        <meta-data
            android:name="com.parse.CLIENT_KEY"
            android:value="@string/back4app_client_key" />

    </application>

</manifest>